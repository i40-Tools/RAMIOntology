                rami:value keys:001 ;
            ]
        ]
    ] ;
    rami:hasSubmodel [
        rami:hasDataSpecification _:sm1
    ] ;
    rami:hasSubmodel [
        rami:hasDataSpecification [
            rami:key [
                rami:type sh:GlobalReference ; #  TODO: select case from enum correct
                rami:local "true"^^xsd:string ;
