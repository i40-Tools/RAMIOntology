            # IdentifierShape:
            rami:idType sh:URI ; 
            rami:id "https://iais.fraunhofer.de/en/aas/examples/devices/raspberrypimodelthreebplus" ;
        ] ;
        rami:assetIdentificationModel _:sm1 ;
    ] ;
    .

_:sec
