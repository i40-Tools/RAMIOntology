obj:clockingSpeed
    a rami:SubmodelElement ;
    rdfs:label "clockingSpeed (frequence)"^^xsd:langString ;
    rdfs:description "The clocking speed of the processor."@en ;
    rami:hasKind sh:Instance ; # FIXME:correct way to select case from enum ?
    rami:idShort "clockingSpeed"^^xsd:string ;
    rami:value "1.4GHz" .
obj:architecture
    a rami:SubmodelElement ;
    rdfs:label "architecture"^^xsd:langString ;
    rdfs:description "The architecture of the processor."@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "architecture"^^xsd:string ;
    rami:value "64-bit" .
obj:socket
    a rami:SubmodelElement ;
    rdfs:label "socket"^^xsd:langString ;
    rdfs:description "The socket of the processor"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "soc"^^xsd:string ;
    rami:value "Broadcom BCM2837B0, Cortex-A53 (ARMv8) 64-bit SoC" .
obj:cpu
    a rami:SubmodelElement ;
    rdfs:label "CPU"^^xsd:langString ;
    rdfs:description "central processing unit"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "processor"^^xsd:string ;
    rami:value "ARM Cortex-A53 CPU" .
obj:ram
    a rami:SubmodelElement ;
    rdfs:label "RAM"^^xsd:langString ;
    rdfs:description "Random Access Memory"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "memory"^^xsd:string ;
    rami:value "1GB LPDDR2 SDRAM" .
obj:wifi
    a rami:SubmodelElement ;
    rdfs:label "WiFi and Bluetooth"^^xsd:langString ;
    rdfs:description "wireless Connectivity"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "wifi"^^xsd:string ;
    rami:value "Dual-band 802.11ac wireless LAN (2.4GHz and 5GHz ) and Bluetooth 4.2" .
obj:ethernet
    a rami:SubmodelElement ;
    rdfs:label "ethernet-connectivity"^^xsd:langString ;
    rdfs:description "wired network connectivity"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "ethernet"^^xsd:string ;
    rami:value "Gigabit Ethernet over USB 2.0 (max 300 Mbps). Power-over-Ethernet support (with separate PoE HAT). Improved PXE network and USB mass-storage booting." .
obj:audio-output
    a rami:SubmodelElement ;
    rdfs:label "audio"^^xsd:langString ;
    rdfs:description "audio output source"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "audioOutput"^^xsd:string ;
    rami:value "built-in-speaker 1W" .
obj:usb-ports
    a rami:SubmodelElement ;
    rdfs:label "usb ports"^^xsd:langString ;
    rdfs:description "Universal serial bus ports count and specification."@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "usb"^^xsd:string ;
    rami:value "4*USB2" .
obj:gpio
    a rami:SubmodelElement ;
    rdfs:label "gpio"^^xsd:langString ;
    rdfs:description "GPIO-Pins"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "gpio"^^xsd:string ;
    rami:value "40-pin" .
obj:power-in
    a rami:SubmodelElement ;
    rdfs:label "power-in"^^xsd:langString ;
    rdfs:description "Voltage/Ampere of the power-in. Supports by PoE."@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "powerIn"^^xsd:string ;
    rami:value "5V/2.5A" .
obj:os
    a rami:SubmodelElement ;
    rdfs:label "OS"^^xsd:langString ;
    rdfs:description "Operating System"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "os"^^xsd:string ;
    rami:value "RaspberianOS (Linux distribution)" .
obj:weight
    a rami:SubmodelElement ;
    rdfs:label "weight"^^xsd:langString ;
    rdfs:description "weigth in gram"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "weigth"^^xsd:string ;
    rami:value "31g" .
obj:heigth
    a rami:SubmodelElement ;
    rdfs:label "heigth"^^xsd:langString ;
    rdfs:description "heigth for suitable cases in mm"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "H"^^xsd:string ;
    rami:value "17 mm" .
obj:length
    a rami:SubmodelElement ;
    rdfs:label "length"^^xsd:langString ;
    rdfs:description "length for suitable cases in mm"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "length"^^xsd:string ;
    rami:value "85 mm" .
obj:witdh
    a rami:SubmodelElement ;
    rdfs:label "width"^^xsd:langString ;
    rdfs:description "width for suitable cases in mm"@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "width"^^xsd:string ;
    rami:value "56 mm" .
obj:maxOutputSpeedUSBPorts
    a rami:SubmodelElement ;
    rdfs:label "maxOutputUSB"^^xsd:langString ;
    rdfs:description "Max. output power of the USB ports."@en ;
    rami:hasKind sh:Instance ;
    rami:idShort "mopp"^^xsd:string ;
    rami:value "5W" .

