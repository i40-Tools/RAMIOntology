aas:describes rdf:type owl:ObjectProperty ;
               rdfs:domain aas:AdminShell ;
               rdfs:range aas:Asset ;
               rdfs:comment "Connect the Asset with the Administration Shell"@en ;
               skos:altLabel "describes"@en .


