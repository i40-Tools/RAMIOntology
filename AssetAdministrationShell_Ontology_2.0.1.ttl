aas:hasReference rdf:type owl:ObjectProperty ;
                  rdfs:domain aas:Submodel ;
                  rdfs:label "has Reference"@en .


###  https://admin-shell.io/aas/2/0/rdf#hasSubjectAttribute
aas:hasSubjectAttribute rdf:type owl:ObjectProperty ;
                         rdfs:comment "A data element that further classifies a specific subject."@en ;
                         rdfs:label "has subjectAttribute"^^xsd:string .


